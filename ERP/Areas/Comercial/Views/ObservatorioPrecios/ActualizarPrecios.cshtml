
@{
    ViewData["Title"] = "ActualizarPrecios";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script src="~/lib/sheetjs/dist/xlsx.full.min.js"></script>
<div class="row justify-content-center">
    <div class="col-xl-4 col-sm-6">
        <div class="card" id="carddigemid">
            <div class="card-header bg-success text-center">
                <h6>LISTA DE PRECIOS</h6>
            </div>
            <div class="card-body text-center">
                <div class="form-group">
                    <label>Lista de precios Digemid</label>
                    <input type="file" id="txtexceldigemid" class="form-control form-control-sm" accept=".xlsx,.xls" />
                </div>
                <div class="form-group">
                    <button type="button" class="btn  btn-primary">
                        # Registros <span class="badge badge-light font-15" id="lblnumregistrosexcel">0</span>
                    </button>
                </div>
                <div class="form-group">
                    <button class="btn btn-lg btn-block btn-success" id="btnsubirlista">Subir lista</button>
                </div>
            </div>
            <div class="card-footer">

            </div>
        </div>
    </div>
    <div class="mt-2 col-xl-12 col-md-12">
        <div class="card">
            <div class="card-body">
                <nav>
                    <div class="nav nav-tabs" id="nav-tab" role="tablist">
                        <a class="nav-link active" id="nav-tab-1" data-toggle="tab" href="#nav1" role="tab">Digemid</a>
                        <a class="nav-link" id="nav-tab-2" data-toggle="tab" href="#nav2" role="tab">Relacion</a>
                        <a class="nav-link" id="nav-tab-3" data-toggle="tab" href="#nav3" role="tab">Productos</a>
                    </div>
                </nav>
                <div class="tab-content" id="nav-tabContent">
                    <div class="tab-pane fade show active" id="nav1" role="tabpanel">

                    </div>
                    <div class="tab-pane fade" id="nav2" role="tabpanel">

                    </div>
                    <div class="tab-pane fade" id="nav3" role="tabpanel">

                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="modal fade" id="modalpartesexcel" data-backdrop="static" tabindex="-1" role="dialog" aria-labelledby="staticBackdropLabel" aria-hidden="true">
    <div class="modal-dialog   modal-dialog-scrollable" role="document">
        <div class="modal-content">
            <div class="modal-header bg-blue">
                <h5 class="modal-title" id="staticBackdropLabel">PARTES DE EXCEL</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body ">
                <div class="card">
                    <div class="card-body ">
                        <div class="contenedor-sticky-500">
                            <table class="table table-sm table-bordered" width="100%">
                                <thead class="thead-dark">
                                    <tr>
                                        <th class="header-sticky">Rango de registros</th>
                                        <th class="header-sticky">Estado</th>
                                        <th class="header-sticky"></th>
                                    </tr>
                                </thead>
                                <tbody id="tbltbodypartesexcel">
                                </tbody>
                            </table>
                        </div>
                    
                    </div>
                </div>


            </div>
            <div class="modal-footer">

            </div>
        </div>
    </div>
</div>
<script src="~/js/modelos/almacen/ProductoDigemid.js"></script>
<script src="~/js/controller/comercial/ObservatorioPrecioController.js"></script>
<script src="~/js/areas/comercial/observatorioprecios/actualizarprecios.js"></script>