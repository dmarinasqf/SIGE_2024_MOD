
@{
    ViewData["Title"] = "Mantenimiento";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<link href="~/lib/datetimepicker/tempusdominus-bootstrap-4.min.css" rel="stylesheet" />
<script src="~/lib/datetimepicker/tempusdominus-bootstrap-4.min.js"></script>
<script>
    var _TURNOS=@Html.Raw(Json.Serialize(ViewBag.Turnos));
</script>
<link href="~/lib/fullCalendar/fullcalendar.min.css" rel="stylesheet" />
<div class="card">
    <div class="card-header">
        <h5><bold>REGISTRAR VISITAS MÉDICAS</bold>  <bold class="text-info font-weight-blod" id="lblnombremedico"></bold></h5>
        <div class="card-toolbar">
            <button data-toggle="tooltip" data-title="Buscar Rep .Medico" id="btnbuscarrepmedico" type="submit" class="card-toolbar-btn btn btn-sm radius-1 btn-dark btn-brc-tp mx-2px">
                <i class="fas fa-search w-2 mx-1px"></i>
            </button>
        </div>
    </div>
    <div class="card-body" id="cardbody" style="min-height:200px">
        <input type="hidden" id="txtidrepmedico" />
        <div class="row justify-content-center">
            <div class="col-xl-12">
                <input type="hidden" value="0" id="omititTurno" />
