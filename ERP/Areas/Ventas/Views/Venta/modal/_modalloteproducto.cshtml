
<div class="modal fade" id="modallotes" data-backdrop="static" tabindex="-1" role="dialog" aria-labelledby="staticBackdropLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg modal-dialog-scrollable" role="document">
        <div class="modal-content">
            <div class="modal-header bg-blue">
                <label class="modal-title" id="staticBackdropLabel">LOTES DEL PRODUCTO  <span id="MBP_lblnombreproductotablastock"></span> (<span id="MBP_lblcodigoproducto"></span>)</label>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body ">
                <div class="card">
                  
                    <div class="card-body">
                        <span class="spinner-grow spinner-grow-sm ocultar" id="MBP_stock_spinner" role="status" aria-hidden="true"></span>
                        <div class="table-responsive">
                            <table class="table table-sm tabla-sticky table-bordered " width="100%" id="MBP_tblstock">
                                <thead class="thead-dark text-center">
                                    <tr>
                                        <th class="header-sticky"></th>
                                        <th class="header-sticky">ALMACEN</th>
                                        <th class="header-sticky">STOCK</th>
                                        <th class="header-sticky">FRACCION</th>
                                        <th class="header-sticky">LOTE</th>
                                        <th class="header-sticky">FV</th>
                                        <th class="header-sticky" @*data-toggle="tooltip" title="Promoción"*@></th>


                                    </tr>
                                </thead>
                                <tbody class="font-12 text-center">
                                </tbody>
                            </table>
                        </div>

                    </div>
                </div>


            </div>
            <div class="modal-footer">

            </div>
        </div>
    </div>
</div>
<script src="~/js/areas/ventas/ventas/modal/modalloteproducto.js"></script>