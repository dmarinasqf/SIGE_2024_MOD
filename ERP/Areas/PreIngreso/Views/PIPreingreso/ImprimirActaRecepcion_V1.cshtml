@model System.Data.DataTable
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <link href="~/css/pagina/impresiones/analisisorganoleptico/analisisorganoleptico.css" rel="stylesheet" />
    <script src="~/lib/vue/vue.min.js"></script>
</head>
<body>
    <script>
        var data = @Html.Raw(JsonConvert.SerializeObject(Model));
        var cabecera = JSON.parse(data[0]['CABECERA']);
        var detalle = JSON.parse(data[0]['DETALLE']);
    </script>
    <div id="aplicacion">
        <div class="row" style="margin-top:10px;margin-left:10px;margin-right:10px;">
            <table width="100%">
                <thead class="">
                    <tr>
                        <th colspan="6" rowspan="3">
                            <div>
                                <img src="~/imagenes/empresas/logo_d.png" height="30" width="180" id="imglogo" />
                            </div>
                        </th>

                        <th colspan="28">FORMATO DE RECEPCION DE PRODUCTOS</th>
                        <th colspan="4">CODIGO</th>
                        <th colspan="6">F.QF.DR.010</th>
                    </tr>
                    <tr>
                        <th colspan="28">TIPO DE DOCUMENTO:Formato</th>
                        <th colspan="4">VERSION</th>
                        <th colspan="6">01</th>
                    </tr>
                    <tr>
                        <th colspan="28">AREA:Drogeria</th>
                        <th colspan="4">PAGINA</th>
                        <th colspan="4">1 de 1</th>
                    </tr>
                    <tr>
                        <th colspan="6" style="text-align:left;">N° DE OC :</th>
                        <th colspan="14">{{cabecera[0]['OC']}}</th>
                        <th colspan="14">VERIFICACIÓN DOCUMENTARIA Y RECEPCÍON</th>
                        <th colspan="1" rowspan="6" style="">CANTIDAD OC</th>
                        @*<th colspan="1" rowspan="6" style="">BULTOS</th>*@
                        <th colspan="1" rowspan="6" style="writing-mode:horizontal-tb">CANTIDAD DEVUELTA</th>
                        <th colspan="4" rowspan="6" style="">OBSERVACION</th>
                    </tr>
                    <tr>
                        <th colspan="6" style="text-align:left;">PROVEEDOR :</th>
                        <th colspan="14">{{cabecera[0]['PROVEEDOR']}}</th>
                        <th colspan="3" rowspan="5">CoA(*)</th>
                        <th colspan="8" rowspan="4">CONDICIONES DE EMBALAJE Y RECEPCION</th>
                        <th colspan="3" rowspan="4">CANT. RECIBIDA</th>
                    </tr>
                    <tr>
                        <th colspan="6" style="text-align:left;">N° DE DOCUMENTO :</th>
                        <th colspan="14">{{cabecera[0]['RUC']}}</th>

                    </tr>
                    <tr>
                        <th colspan="6" style="text-align:left;">FECHA DE RECEPCÍON :</th>
                        <th colspan="14">{{cabecera[0]['FECHA_R']}}</th>
                    </tr>
                    <tr>
                        <th colspan="1" rowspan="2">Nº</th>
                        <th colspan="5" rowspan="2" align="center">DESCRIPCIÓN DEL PRODUCTO</th>
                        <th colspan="7" rowspan="2">FABRICANTE</th>
                        <th colspan="4" rowspan="2">LOTE</th>
                        <th colspan="3" rowspan="2">VENC.</th>
                    </tr>
                    <tr>
                        <th colspan="1">1</th>
                        <th colspan="1">2</th>
                        <th colspan="1">3</th>
                        <th colspan="1">4</th>
                        <th colspan="2">Temp.</th>
                        <th colspan="3">Und</th>
                    </tr>

                </thead>
                <tbody v-html="tbldetalleao">
                </tbody>
            </table>
        </div>
        <div style="margin-left:10px;margin-right:10px;margin-top:30px;">
            <table style="border: 0px solid black; border-bottom: 1px solid black;" width="100%">
                <thead>
                    <tr>
                        <td colspan="5" class="Without-Border">
                            <img src="~/imagenes/firmas/almacen/Henry_almacen.png" width="100">
                        </td>
                        <td colspan="3" class="text-center" style="border:0px;width:100px"></td>
                        <td colspan="4" class="text-center" style="border:0px;width:100px"></td>
                        <td colspan="5" class="text-center" style="border:0px;width:100px">
                            <img src="~/imagenes/firmas/almacen/JIE.png" width="100">
                        </td>
                    </tr>
                    <tr>
                        <th colspan="5" class="text-center BorderTop">RESPONSABLE DE LA RECEPCION </th>
                        <th colspan="3" class="text-center" style="border:0px;width:100px"></th>
                        <th colspan="4" class="text-center" style="border:0px;width:100px"></th>
                        <th colspan="5" class="text-center BorderTop">VERIFICADO POR</th>
                    </tr>
                    <tr style="height:50px"></tr>

                    <tr>
                        <th colspan="34">LEYENDA</th>

                    </tr>
                    <tr>
                        <th colspan="5" class="text-left">
                            Registro de conformidad:
                            <ul class="text-left">
                                <li>(✓) Conforme</li>
                                <li>(X) No Conforme</li>
                                <li></li>
                            </ul>
                            <ul class="text-left">
                                <li>(*)CoA:Certificado de Análisis,Protocolo de Análisis,</li>
                                <li>o Especif. Técnicas de los Caracteres Físicos.</li>
                                <li></li>
                            </ul>
                        </th>
                        <th colspan="9" class="text-left">
                            <ul class="text-left">
                                Condición de Embalaje:
                                <li>1: No Abierto</li>
                                <li>2: Limpio</li>
                                <li>3: No Arrugado</li>
                                <li>4: No Quebrado o Húmedo</li>
                                <li></li>
                            </ul>                        
                        </th>
                        <th class="text-left">
                            Temp. Recepc.: TEMPERATURA DE RECEPCION
                            <ul class="text-left">
                                <li></li>
                                <li>(**)Registro de Temperatura de Recepcion:</li>
                                <li>TA: TEMPERATURA AMBIENTE(15°C a 30°C)</li>
                                <li>TC: TEMPERATURA CONTROLADA(15°C a 25°C)</li>
                            </ul>
                        </th>
                    </tr>
                </thead>
            </table>
        </div>
    </div>
</body>
</html>
<script src="~/js/areas/preingreso/preingreso/actarecepcion_V1.js"></script>